{
  "env": {
    "es2020": true,
    "node": true,
    "jest": true,
    "shelljs": true
  },
  "extends": ["airbnb-base"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 11
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "no-use-before-define": "warn",
    "no-undef": "warn",
    "class-methods-use-this": "off"
  }
}
